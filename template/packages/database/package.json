{
  "name": "@workspace/database",
  "version": "1.0.0",
  "exports": {
    ".": "./src/client.ts"
  },
  "scripts": {
    "db:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:push": "prisma db push --schema=./prisma/schema.prisma",
    "db:seed": "tsx ./prisma/seed.ts",
    "dev": "tsup src/* --watch",
    "format": "prisma format --schema=./prisma/schema.prisma",
    "generate": "prisma generate --schema=./prisma/schema.prisma",
    "lint": "eslint . --max-warnings 0",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "studio": "prisma studio --schema=./prisma/schema.prisma"
  },
  "dependencies": {
    "@brakebein/prisma-generator-nestjs-dto": "^1.29.0",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "eslint": "latest",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "4.19.1",
    "typescript": "5.5.4"
  }
}
